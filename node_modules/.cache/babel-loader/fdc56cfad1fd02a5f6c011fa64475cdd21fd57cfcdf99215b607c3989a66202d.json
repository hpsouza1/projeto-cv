{"ast":null,"code":"import axios from 'axios';\nimport SearchComponent from './components/SearchComponent.vue';\nimport RepositoryList from './components/RepositoryList.vue';\nexport default {\n  name: 'App',\n  components: {\n    SearchComponent,\n    RepositoryList\n  },\n  data() {\n    return {\n      repositories: [],\n      darkMode: false // Inicialmente, o modo noturno está desativado\n    };\n  },\n  methods: {\n    toggleDarkMode() {\n      this.darkMode = !this.darkMode;\n    },\n    async fetchRepositories(query) {\n      try {\n        const response = await axios.get(`https://api.github.com/search/repositories?q=${query}`);\n        this.repositories = response.data.items.map(repo => ({\n          id: repo.id,\n          name: repo.full_name,\n          description: repo.description,\n          avatar: repo.owner.avatar_url\n        }));\n        console.log('Repositórios carregados:', this.repositories);\n      } catch (error) {\n        console.error('Erro ao buscar repositórios:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","SearchComponent","RepositoryList","name","components","data","repositories","darkMode","methods","toggleDarkMode","fetchRepositories","query","response","get","items","map","repo","id","full_name","description","avatar","owner","avatar_url","console","log","error"],"sources":["src/App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\" :class=\"{'dark-mode': darkMode}\">\r\n    <header class=\"header\">\r\n      <img\r\n        :src=\"darkMode ? require('./assets/logo-dark.svg') : require('')\"\r\n        alt=\"Logo\"\r\n        class=\"header-logo\"\r\n      />\r\n      <button @click=\"toggleDarkMode\" class=\"header-dark-mode-button\">\r\n        {{ darkMode ? \"Desativar Modo Noturno\" : \"Ativar Modo Noturno\" }}\r\n      </button>\r\n    </header>\r\n    \r\n    <SearchComponent @search=\"fetchRepositories\" />\r\n    <RepositoryList \r\n      v-if=\"repositories.length\" \r\n      :repos=\"repositories\" \r\n      @repository-selected=\"showRepositoryDetails\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport SearchComponent from './components/SearchComponent.vue';\r\nimport RepositoryList from './components/RepositoryList.vue';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    SearchComponent,\r\n    RepositoryList,\r\n  },\r\n  data() {\r\n    return {\r\n      repositories: [],\r\n      darkMode: false, // Inicialmente, o modo noturno está desativado\r\n    };\r\n  },\r\n  methods: {\r\n    toggleDarkMode() {\r\n      this.darkMode = !this.darkMode;\r\n    },\r\n    async fetchRepositories(query) {\r\n      try {\r\n        const response = await axios.get(\r\n          `https://api.github.com/search/repositories?q=${query}`\r\n        );\r\n        this.repositories = response.data.items.map(repo => ({\r\n          id: repo.id,\r\n          name: repo.full_name,\r\n          description: repo.description,\r\n          avatar: repo.owner.avatar_url,\r\n        }));\r\n        console.log('Repositórios carregados:', this.repositories);\r\n      } catch (error) {\r\n        console.error('Erro ao buscar repositórios:', error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.app-background {\r\n  background-image: url(\"./assets/github-background.svg\");\r\n  background-size: 50%;\r\n  background-repeat: no-repeat;\r\n  background-position: right;\r\n  min-height: 100vh;\r\n  display: flex;\r\n  justify-content: top;\r\n  align-items: flex-start;\r\n  flex-direction: column;\r\n  padding: 20px; /* Espaçamento das bordas */\r\n  color: var(--title-color);\r\n}\r\n\r\n.header {\r\n  background-color: var(--header-background-color);\r\n  padding: 10px;\r\n  display: flex;\r\n  justify-content: space-evenly; /* Ajustando a posição dos elementos */\r\n  align-items: center;\r\n  width: 100%;\r\n}\r\n\r\n.header-logo {\r\n  max-width: 150px;\r\n  height: auto;\r\n}\r\n\r\n.header-title {\r\n  margin: 0;\r\n  font-size: 1.5rem;\r\n  color: var(--title-color);\r\n}\r\n\r\n.header-dark-mode-button {\r\n  padding: 5px;\r\n  background-color: var(--button-color);\r\n  border: 1px black none;\r\n  border-radius: 4px;\r\n  font-size: 1rem;\r\n  cursor: pointer;\r\n  color: var(--text-color);\r\n}\r\n\r\n.header-dark-mode-button:hover {\r\n  background-color: var(--button-hover-color); /* Cor do botão ao passar o mouse */\r\n}\r\n</style>\r\n"],"mappings":"AAuBA,OAAAA,KAAA;AACA,OAAAC,eAAA;AACA,OAAAC,cAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAH,eAAA;IACAC;EACA;EACAG,KAAA;IACA;MACAC,YAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACAC,eAAA;MACA,KAAAF,QAAA,SAAAA,QAAA;IACA;IACA,MAAAG,kBAAAC,KAAA;MACA;QACA,MAAAC,QAAA,SAAAZ,KAAA,CAAAa,GAAA,CACA,gDAAAF,KAAA,EACA;QACA,KAAAL,YAAA,GAAAM,QAAA,CAAAP,IAAA,CAAAS,KAAA,CAAAC,GAAA,CAAAC,IAAA;UACAC,EAAA,EAAAD,IAAA,CAAAC,EAAA;UACAd,IAAA,EAAAa,IAAA,CAAAE,SAAA;UACAC,WAAA,EAAAH,IAAA,CAAAG,WAAA;UACAC,MAAA,EAAAJ,IAAA,CAAAK,KAAA,CAAAC;QACA;QACAC,OAAA,CAAAC,GAAA,kCAAAlB,YAAA;MACA,SAAAmB,KAAA;QACAF,OAAA,CAAAE,KAAA,iCAAAA,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}